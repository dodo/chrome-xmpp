{"name": "XMPP - core",
 "description": "The missing XMPP Gear for Browsers",
 "version": "0.1.0",

 "manifest_version": 2,
 "app": {
   "background": {
      "scripts": ["background.js"],
      "persistent": true
   }
 },
 "permissions": [ {"socket": ["tcp-connect:*", "udp-send-to", "udp-bind"]} ],
 "icons": { "16": "favicon.png",  "64": "icon.png" }
}